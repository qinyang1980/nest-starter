{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "启动",
      "type": "node",
      "request": "launch",
      "protocol": "inspector",
      "program": "${workspaceRoot}/index.js",
      "stopOnEntry": false,
      "args": [],
      "cwd": "${workspaceRoot}",
      "preLaunchTask": null,
      "runtimeExecutable": null,
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "sourceMaps": true,
      "outFiles": [
        "${workspaceRoot}/build/dist/**/*.js"
      ]
    },
    {
      "name": "附加",
      "type": "node",
      "request": "attach",
      "port": 5858,
      "address": "localhost",
      "restart": false,
      "sourceMaps": true,
      "localRoot": "${workspaceRoot}",
      "remoteRoot": null,
      "outFiles": [
        "${workspaceRoot}/build/dist/**/*.js"
      ]
    },
    {
      "name": "附加到进程",
      "type": "node",
      "request": "attach",
      "processId": "${command:PickProcess}",
      "port": 5858,
      "sourceMaps": true,
      "outFiles": [
        "${workspaceRoot}/build/dist/**/*.js"
      ]
    }
  ]
}